[alias]
	aa = add --all
	br = branch
	ci = commit
	co = checkout
	df = diff
	st = status
	uc = checkout --
	change-commits = "!f() { VAR=$1; OLD=$2; NEW=$3; shift 3; git filter-branch --env-filter \"if [[ \\\"$`echo $VAR`\\\" = '$OLD' ]]; then export $VAR='$NEW'; fi\" $@; }; f"
[core]
	excludesFile = /Users/bram/.gitignore
[color]
	ui = true
[merge]
	log = true
	tool = vimdiff
[push]
	default = current
[user]
	name = Bram Rohde
  email = bram@br4m.nl
[includeIf "gitdir:/Users/bram/Developer/radboud/"]
  path = /Users/bram/Developer/radboud/.gitconfig
[init]
	defaultBranch = main
