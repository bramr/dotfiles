// Place your settings in this file to overwrite the default settings
{
  "editor.fontFamily": "Hack, Menlo, Monaco, 'Courier New', monospace",
  "editor.lineNumbers": "relative",
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[java]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[shellscript]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#333333",
  "vim.statusBarColors.insert": "#d33682",
  "vim.statusBarColors.visual": "#268bd2",
  "vim.statusBarColors.visualline": "#2aa198",
  "vim.statusBarColors.visualblock": "#859900",
  "vim.statusBarColors.replace": "#b58900",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["Tab"],
      "after": ["C-w"]
    },
    {
      "before": ["K"],
      "after": ["1", "0", "k"]
    },
    {
      "before": [" "],
      "after": ["1", "0", "j"]
    },
    {
      "before": ["J"],
      "after": ["1", "0", "j"]
    }
  ],
  "php.suggest.basic": false,
  "phpcs.standard": "PSR1,PSR2",
"window.zoomLevel": 1,
  "workbench.colorTheme": "Monokai GRS",
  "workbench.iconTheme": "vs-seti",
  "vsicons.dontShowNewVersionMessage": true,
  "zenMode.hideStatusBar": false,
"workbench.colorCustomizations": {
    "statusBar.background": "#333333",
    "statusBar.noFolderBackground": "#333333",
    "statusBar.debuggingBackground": "#333333"
},
  "editor.minimap.enabled": false,
  "java.home": "/Library/Java/JavaVirtualMachines/jdk1.8.0_152.jdk/Contents/Home",
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  "java.format.settings.profile": "GoogleStyle",
  "java.format.enabled": true,
  "editor.useTabStops": false,
"editor.detectIndentation": false,
"editor.renderWhitespace": "all",
"window.nativeTabs": true,
"editor.suggestSelection": "first",
"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue"
}